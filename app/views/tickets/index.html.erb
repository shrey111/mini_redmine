<h1>Listing tickets</h1>
<div class="table-responsive">
<table class="table">
<thead>
  <tr>
    <th >Status</th>
    <th>User</th>
    <th>Title</th>
    <th>Priority</th>
    <th>Description</th>
    <th>Type</th>
    <th>Project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>
<% @tickets.each do |ticket| %>
  <tr>
    <td><%= ticket.status %></td>
    <td><%= ticket.user_id %></td>
    <td><%= ticket.title %></td>
    <td><%= ticket.priority %></td>
    <td><%= ticket.description %></td>
    <td><%= ticket.type %></td>
    <td><%= ticket.project_id %></td>
    <td><%= link_to 'Show', ticket %></td>
    <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
    <td><%= link_to 'Destroy', ticket, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>

</table>
</div>
<br />

<%= link_to 'New Ticket', new_ticket_path ,style: "color:darkblue"%>
